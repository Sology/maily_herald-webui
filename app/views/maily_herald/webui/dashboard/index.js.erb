$(".dashboard-periods a").removeClass("active");
$(".period-<%= params[:period] || "month" %>").addClass("active");

<%= smart_listing_update(:logs) %>
<%= smart_listing_update(:schedules) %>

$(".history-graph").data("delivered", <%= history_graph_data[:delivered].html_safe %>);
$(".history-graph").data("opened", <%= history_graph_data[:opened].html_safe %>);
$(".history-graph").data("clicked", <%= history_graph_data[:clicked].html_safe %>);
$(".history-graph").data("error", <%= history_graph_data[:error].html_safe %>);
<% if settings.show_skipped? %>
  $(".history-graph").data("skipped", <%= history_graph_data[:skipped].html_safe %>);
<% end %>
$(".history-graph").historyGraph();

$(".well.row.statusbox .col-sm-12").html("<%= escape_javascript(render(partial: "filter_logs_nav_by_status")) %>");
$("span.dashboard-periods").html("<%= escape_javascript(render(partial: "filter_logs_nav_by_period")) %>");
